{
  "name": "herodotus-cloud-ui",
  "private": false,
  "version": "3.5.8.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git add . && git status && git-cz",
    "push": "pnpm run commit && git push --set-upstream origin develop",
    "dev": "pnpm run --filter=@herodotus/ui dev",
    "prod": "pnpm run --filter=@herodotus/ui prod",
    "shared:build": "pnpm -r --filter=@herodotus-cloud/* run build",
    "pub:changeset": "pnpm changeset",
    "pub:alpha": "pnpm changeset pre enter alpha",
    "pub:beta": "pnpm changeset pre enter beta",
    "pub:rc": "pnpm changeset pre enter rc",
    "pub:exit-pre": "pnpm changeset pre exit",
    "pub:version": "pnpm changeset version",
    "pub:release": "pnpm changeset publish"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky-run": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|json|md|scss|ts|vue)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "disable-devtool": "^0.3.9",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "quasar": "^2.18.6",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "rollup": "4.53.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.93.3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "unocss": "66.5.9",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vconsole": "^3.15.1",
    "vite": "^7.2.4",
    "vite-plugin-compression2": "^2.3.1",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.5"
  }
}
