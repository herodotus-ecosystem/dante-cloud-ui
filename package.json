{
  "name": "dante-cloud-ui",
  "private": true,
  "version": "3.4.4.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git add . && git status && git-cz",
    "push": "pnpm run commit && git push --set-upstream origin develop",
    "dev": "pnpm run --filter=@dante/ui dev",
    "prod": "pnpm run --filter=@dante/ui prod",
    "shared:build": "pnpm -r --filter=@herodotus/* run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky-run": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|json|md|scss|ts|vue)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.16.17",
    "disable-devtool": "^0.3.8",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "qs": "^6.14.0",
    "quasar": "^2.18.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.23.0",
    "@quasar/app-vite": "^2.2.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@types/qs": "^6.9.18",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "rollup": "4.39.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.86.2",
    "terser": "^5.39.0",
    "typescript": "^5.8.2",
    "unocss": "66.1.0-beta.8",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "vconsole": "^3.15.1",
    "vite": "^6.2.4",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-dts": "4.5.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
