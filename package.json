{
  "name": "dante-cloud-ui",
  "private": true,
  "version": "3.5.7.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git add . && git status && git-cz",
    "push": "pnpm run commit && git push --set-upstream origin develop",
    "dev": "pnpm run --filter=@dante/ui dev",
    "prod": "pnpm run --filter=@dante/ui prod",
    "shared:build": "pnpm -r --filter=@herodotus/* run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky-run": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|json|md|scss|ts|vue)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "disable-devtool": "^0.3.9",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "qs": "^6.14.0",
    "quasar": "^2.18.5",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.38.0",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.9.2",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.6",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "rollup": "4.52.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.93.2",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "unocss": "66.5.4",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vconsole": "^3.15.1",
    "vite": "^7.1.12",
    "vite-plugin-compression2": "^2.3.1",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.2"
  }
}
