{
  "name": "dante-cloud-ui",
  "private": true,
  "version": "3.5.1.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git add . && git status && git-cz",
    "push": "pnpm run commit && git push --set-upstream origin develop",
    "dev": "pnpm run --filter=@dante/ui dev",
    "prod": "pnpm run --filter=@dante/ui prod",
    "shared:build": "pnpm -r --filter=@herodotus/* run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky-run": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|json|md|scss|ts|vue)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "disable-devtool": "^0.3.8",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "qs": "^6.14.0",
    "quasar": "^2.18.1",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.29.0",
    "@quasar/app-vite": "^2.2.1",
    "@quasar/vite-plugin": "^1.9.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.3",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "rollup": "4.44.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-embedded": "^1.89.2",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "unocss": "66.2.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.7.0",
    "vconsole": "^3.15.1",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-compression2": "^2.0.1",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  }
}
